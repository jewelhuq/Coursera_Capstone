 "cells": [
  {
   "cell_type": "code",
   "execution_count": 133,
   "metadata": {},
   "outputs": [],
   "source": [
    "import urllib.request\n",
    "wp = urllib.request.urlopen(\"https://en.wikipedia.org/wiki/List_of_postal_codes_of_Canada:_M\")\n",
    "page = wp.read()\n",
    "from bs4 import BeautifulSoup\n",
    "soup = BeautifulSoup(page)\n",
    "first_table = soup.find(\"table\", class_ = \"wikitable sortable\")\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 134,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "A=[]\n",
    "B=[]\n",
    "C=[]\n",
    "\n",
    "\n",
    "for row in first_table.findAll(\"tr\"):\n",
    "    cells = row.findAll('td')\n",
    "   \n",
    "    if(len(cells)==3):\n",
    "      # print(cells[0])\n",
    "    \n",
    "        borough=cells[1].find(text=True)\n",
    "        neighborhood=cells[2].find(text=True)\n",
    "        neighborhood=neighborhood.strip()\n",
    "        borough=borough.strip()\n",
    "        \n",
    "        if(borough!=\"Not assigned\"):\n",
    "                \n",
    "                    \n",
    "                    A.append(cells[0].find(text=True))\n",
    "                    B.append(borough)\n",
    "                    if(neighborhood ==\"Not assigned\"):\n",
    "                        C.append(borough)\n",
    "                    else:\n",
    "                        C.append(neighborhood)    \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 135,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "df=pd.DataFrame()\n",
    "df['PostalCode']=A\n",
    "df['Borough']=B\n",
    "df['Neighborhood']=C\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 139,
   "metadata": {},
   "outputs": [],
   "source": [
    "result= df.groupby('PostalCode').agg(lambda x: ','.join(set(x))).reset_index()\n"
   ]
  },
